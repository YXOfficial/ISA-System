spawn(function()
    local char = game.Players.LocalPlayer.Character
    local slashcolor1 = Instance.new('Color3Value')
    slashcolor1.Parent = game.Players.LocalPlayer.Backpack
    slashcolor1.Name = "corrupted"
    local tickSpeed = 10; 
    task.spawn(function()
        while wait() do
            local h = tick() % tickSpeed / tickSpeed
            local c = Color3.fromHSV(h,1,1);

            slashcolor1.Value = c
        end
    end)
    -- tween to 0.8

    spawn(function()
    local ti = TweenInfo.new(
        2.5, -- Time
        Enum.EasingStyle.Sine, -- EasingStyle
        Enum.EasingDirection.InOut, -- EasingDirection
        -1, -- RepeatCount (when less than zero the tween will loop indefinitely)
        true, -- Reverses (tween will reverse once reaching it's goal)
        0.5 -- DelayTime
    )

    local tween1 = game:GetService('TweenService'):Create(slashcolor1, ti, {Value = Color3.fromHSV(0.8,1,1)})
    tween1:Play()
    end)
    wait()

    wfc = game:GetService("Players").LocalPlayer.Backpack:WaitForChild('Main')
    for i,v in pairs(wfc:GetDescendants()) do
        if v.Name:match("Moves") then
        test = v
    end
    end
    test1 = test['ModuleScript']['Animations']
    for i,v in pairs(test:GetDescendants()) do
          if v.Name:match("Light") then
        test = v
    end
    end

    rotval = 0
while true do wait(0.1)
        rotval = rotval + 20
        if rotval >= 360 then rotval = 0 end
    local A_1 = {
                    [1] = getrenv()._G.Pass, 
                    [2] = "SlashEffect", 
                    [3] = test, 
                    [4] = slashcolor1.Value, 
                    [5] = CFrame.new(0,3,0)*CFrame.Angles(math.rad(0),math.rad(rotval),math.rad(0))
              }
              local Event = game:GetService("ReplicatedStorage").Remotes.Events
              Event:FireServer(A_1)
    end
    end)

local mouse = game.Players.LocalPlayer:GetMouse()
