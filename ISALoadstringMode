if ischatting == false then
ischatting = true
Chat_Toggle_Label.Text = "~True~"
spawn(function()
game.Players.LocalPlayer.Chatted:connect(function(msg)
for i = 0, msg:len(), 1 do
	text.Text = "~"..msg:sub(0, i).."~"
end

text.Text = "~Fell Apart AF~"
end)
end)
else
ischatting = false
Chat_Toggle_Label.Text = "~False~"
end
end)
