if NoClothes then
pchar = game.Players.LocalPlayer.Character
if pchar:FindFirstChild("Shirt") then
   pchar.Shirt:Remove()
end
if pchar:FindFirstChild("Pants") then
   pchar.Pants:Remove()
end
